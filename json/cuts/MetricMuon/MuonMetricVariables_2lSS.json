    ["define", "leadingBJet_mass", "BJet_mass[0]"],
    ["define", "subleadingBJet_pt", "BJet_pt[1]"],
    ["define", "subleadingBJet_eta", "BJet_eta[1]"],
    ["define", "subleadingBJet_phi", "BJet_phi[1]"],
    ["define", "subleadingBJet_mass", "BJet_mass[1]"]
]